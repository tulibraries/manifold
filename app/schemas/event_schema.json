{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "data"
  ],
  "properties": {
    "data": {
      "$id": "#/properties/data",
      "type": "object",
      "title": "The Data Schema",
      "required": [
        "id",
        "type",
        "attributes",
        "links"
      ],
      "properties": {
        "id": {
          "$id": "#/properties/data/properties/id",
          "type": "string",
          "title": "The Id Schema",
          "default": "",
          "examples": [
            "1"
          ],
          "pattern": "^(.*)$"
        },
        "type": {
          "$id": "#/properties/data/properties/type",
          "type": "string",
          "title": "The Type Schema",
          "default": "event",
          "examples": [
            "building",
            "person",
            "event"
          ],
          "pattern": "^(.*)$"
        },
        "attributes": {
          "$id": "#/properties/data/properties/attributes",
          "type": "object",
          "title": "The Attributes Schema",
          "required": [],
          "properties": {
            "title": {
              "$id": "#/properties/data/properties/attributes/properties/title",
              "type": "string",
              "title": "The Title Schema",
              "default": "",
              "examples": [
                "2nd Annual Winter Concert"
              ],
              "pattern": "^(.*)$"
            },
            "label": {
              "$id": "#/properties/data/properties/attributes/properties/label",
              "type": "string",
              "title": "The Label Schema",
              "default": "",
              "examples": [
                "2nd Annual Winter Concert"
              ],
              "pattern": "^(.*)$"
            },
            "start_time": {
              "$id": "#/properties/data/properties/attributes/properties/start_time",
              "type": "string",
              "title": "The Start_time Schema",
              "default": "",
              "examples": [
                "2018-12-05T12:15:00.000Z"
              ],
              "pattern": "^(.*)$"
            },
            "end_time": {
              "$id": "#/properties/data/properties/attributes/properties/end_time",
              "type": "string",
              "title": "The End_time Schema",
              "default": "",
              "examples": [
                "2018-12-05T12:15:00.000Z"
              ],
              "pattern": "^(.*)$"
            },
            "cancelled": {
              "$id": "#/properties/data/properties/attributes/properties/cancelled",
              "type": "boolean",
              "title": "The Cancelled Schema",
              "default": false,
              "examples": [
                false
              ]
            },
            "registration_status": {
              "$id": "#/properties/data/properties/attributes/properties/registration_status",
              "type": "boolean",
              "title": "The Registration_status Schema",
              "default": false,
              "examples": [
                true
              ]
            },
            "registration_link": {
              "$id": "#/properties/data/properties/attributes/properties/registration_link",
              "type": "string",
              "title": "The Registration_link Schema",
              "default": "",
              "examples": [
                "#"
              ],
              "pattern": "^(.*)$"
            },
            "content_hash": {
              "$id": "#/properties/data/properties/attributes/properties/content_hash",
              "type": "string",
              "title": "The Content_hash Schema",
              "default": "",
              "examples": [
                "4b75124fd9ec91da03b45532de5febd0aebab00c"
              ],
              "pattern": "^(.*)$"
            },
            "alt_text": {
              "$id": "#/properties/data/properties/attributes/properties/alt_text",
              "type": "string",
              "title": "The Alt_text Schema",
              "default": "",
              "examples": [
                "2nd Annual Winter Concert at Ginsburg library"
              ],
              "pattern": "^(.*)$"
            },
            "ensemble_identifier": {
              "$id": "#/properties/data/properties/attributes/properties/ensemble_identifier",
              "type": "string",
              "title": "The Ensemble_identifier Schema",
              "default": "",
              "examples": [
                "0NnOvtHCoUKvUxJj6BVajg"
              ],
              "pattern": "^(.*)$"
            },
            "tags": {
              "$id": "#/properties/data/properties/attributes/properties/tags",
              "type": "string",
              "title": "The Tags Schema",
              "default": "",
              "examples": [ "" ],
              "pattern": "^(.*)$"
            },
            "all_day": {
              "$id": "#/properties/data/properties/attributes/properties/all_day",
              "type": "boolean",
              "title": "The All_day Schema",
              "default": false,
              "examples": [
                false
              ]
            },
            "image": {
              "$id": "#/properties/data/properties/attributes/properties/image",
              "type": "string",
              "title": "The Image Schema",
              "default": "",
              "examples": [
                "http://test.host/rails/active_storage/blobs/eyJfcmFpbHMiOnsibWVzc2FnZSI6IkJBaHBBWlk9IiwiZXhwIjpudWxsLCJwdXIiOiJibG9iX2lkIn19--d12640749274b208e7e27c788f8dab20ad17cab0/winter_concert_square.jpg"
              ],
              "pattern": "^(.*)$"
            },
            "space": {
              "$id": "#/properties/data/properties/attributes/properties/space",
              "type": "string",
              "title": "The Space Schema",
              "default": "",
              "examples": [
                "Ginsburg Room 243"
              ],
              "pattern": "^(.*)$"
            },
            "address1": {
              "$id": "#/properties/data/properties/attributes/properties/address1",
              "type": "string",
              "title": "The Address1 Schema",
              "default": "",
              "examples": [
                "Medical Education and Research Building, 3500 N. Broad Street"
              ],
              "pattern": "^(.*)$"
            },
            "address2": {
              "$id": "#/properties/data/properties/attributes/properties/address2",
              "type": "string",
              "title": "The Address2 Schema",
              "default": "",
              "examples": [
                "Philadelphia, PA 19140"
              ],
              "pattern": "^(.*)$"
            },
            "contact_name": {
              "$id": "#/properties/data/properties/attributes/properties/contact_name",
              "type": "string",
              "title": "The Contact_name Schema",
              "default": "",
              "examples": [
                "Stephanie Roth"
              ],
              "pattern": "^(.*)$"
            },
            "contact_email": {
              "$id": "#/properties/data/properties/attributes/properties/contact_email",
              "type": "string",
              "title": "The Contact_email Schema",
              "default": "",
              "examples": [
                "stephanie.roth@temple.edu"
              ],
              "pattern": "^(.*)$"
            },
            "contact_phone": {
              "$id": "#/properties/data/properties/attributes/properties/contact_phone",
              "type": "string",
              "title": "The Contact_phone Schema",
              "default": "",
              "examples": [
                "2157079469"
              ],
              "pattern": "^(.*)$"
            },
            "thumbnail_image": {
              "$id": "#/properties/data/properties/attributes/properties/thumbnail_image",
              "type": "string",
              "title": "The Thumbnail_image Schema",
              "default": "",
              "examples": [
                "http://test.host/rails/active_storage/representations/eyJfcmFpbHMiOnsibWVzc2FnZSI6IkJBaHBBWlk9IiwiZXhwIjpudWxsLCJwdXIiOiJibG9iX2lkIn19--d12640749274b208e7e27c788f8dab20ad17cab0/eyJfcmFpbHMiOnsibWVzc2FnZSI6IkJBaDdCam9MY21WemFYcGxTU0lNTVRBd2VERXdNQVk2QmtWVSIsImV4cCI6bnVsbCwicHVyIjoidmFyaWF0aW9uIn19--1780aa09167cc24cd4beae09f819f90d80b64c3a/winter_concert_square.jpg"
              ],
              "pattern": "^(.*)$"
						},
						"updated_at": {
							"$id": "#/properties/data/properties/attributes/properties/updated_at",
							"type": "string",
							"title": "The time the event record was updated",
 							"format": "date-time",
							"default": "",
							"examples": [
								"2018-01-01T12:00:00"
							]
            }
          }
        },
        "links": {
          "$id": "#/properties/data/properties/links",
          "type": "object",
          "title": "The Links Schema",
          "required": [
            "self"
          ],
          "properties": {
            "self": {
              "$id": "#/properties/data/properties/links/properties/self",
              "type": "string",
              "title": "The Self Schema",
              "default": "",
              "examples": [
                "http://test.host/beyondthepage/1"
              ],
              "pattern": "^(.*)$"
            }
          }
        }
      }
    }
  }
}
