<%= f.input :book_title, input_html: { value: params[:book_title] }, required: :true, 
				label: t("manifold.forms.fields.title") %>
<%= f.input :call_number, input_html: { value: params[:call_number] }, 
				label: t("manifold.forms.fields.call_number"), 
				hint: t("manifold.forms.fields.call_number"), 
				required: :true %>
<%= f.input :pickup_location, as: :select, 
				collection: t("manifold.forms.fields.pickup_location_options").map{ |k,v| v }, 
				label: t("manifold.forms.fields.pickup_location_label"), 
				required: :true %>